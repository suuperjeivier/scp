package dto.horario;

import dto.listados.TipoEstatusDTO;

public class ConfiguracionHorarioDTO {
	private int idConfiguracionHorario;
	private int tiempoValidezHorario;
	private TipoEstatusDTO estatusDTO;
	
	public int getIdConfiguracionHorario() {
		return idConfiguracionHorario;
	}
	
	public void setIdConfiguracionHorario(int idConfiguracionHorario) {
		this.idConfiguracionHorario = idConfiguracionHorario;
	}

	public int getTiempoValidezHorario() {
		return tiempoValidezHorario;
	}

	public void setTiempoValidezHorario(int tiempoValidezHorario) {
		this.tiempoValidezHorario = tiempoValidezHorario;
	}

	public TipoEstatusDTO getEstatusDTO() {
		return estatusDTO;
	}

	public void setEstatusDTO(TipoEstatusDTO estatusDTO) {
		this.estatusDTO = estatusDTO;
	}

}
