package dto.listados;

public class EstadoCivilDTO {
	private int idEstadoCivil;
	private String nombreEstadoCivil;
	private GeneroDTO generoDTO;
	
	public int getIdEstadoCivil() {
		return idEstadoCivil;
	}
	
	public void setIdEstadoCivil(int idEstadoCivil) {
		this.idEstadoCivil = idEstadoCivil;
	}

	public String getNombreEstadoCivil() {
		return nombreEstadoCivil;
	}

	public void setNombreEstadoCivil(String nombreEstadoCivil) {
		this.nombreEstadoCivil = nombreEstadoCivil;
	}

	public GeneroDTO getGeneroDTO() {
		return generoDTO;
	}

	public void setGeneroDTO(GeneroDTO generoDTO) {
		this.generoDTO = generoDTO;
	}

}
