package dto.bitacora;

import java.util.Vector;

import herramientas.FechaDTO;

public class BitacoraFechaDTO {

	private FechaDTO fecha;
	private Vector<BitacoraDTO> registrosEnFecha;
	
	public BitacoraFechaDTO(){
		if(this.getFecha() == null){
			this.setFecha(new FechaDTO());
		}
	}
	
	public FechaDTO getFecha() {
		return fecha;
	}
	public void setFecha(FechaDTO fecha) {
		this.fecha = fecha;
	}
	public Vector<BitacoraDTO> getRegistrosEnFecha() {
		return registrosEnFecha;
	}
	public void setRegistrosEnFecha(Vector<BitacoraDTO> registrosEnFecha) {
		this.registrosEnFecha = registrosEnFecha;
	}
	
}
