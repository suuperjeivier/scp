html {
  position: relative;
  min-height: 100%;
}
body {
  /* Margin bottom by footer height */
  margin-bottom: 77px;
}
.footer {
  position: absolute;
  bottom: 0;
  width: 100%;
  /* Set the fixed height of the footer here */
  height: 77px;
  background-color: #2e2e2e;
  line-height: 60px;
}
.modal { overflow: visible; }
.modal-body { overflow-y: visible !important; }

