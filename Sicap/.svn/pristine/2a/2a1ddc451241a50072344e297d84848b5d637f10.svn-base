package dto.ordenes;

public class OrdenTecnicoDTO {
	private OrdenDTO ordenDTO;
	private TecnicoDTO tecnicoDTO;
	private Double viatico;
	
	public OrdenTecnicoDTO(){
		setOrdenDTO(new OrdenDTO());
		setTecnicoDTO(new TecnicoDTO());		
	}

	/**
	 * @return the ordenDTO
	 */
	public OrdenDTO getOrdenDTO() {
		return ordenDTO;
	}

	/**
	 * @param ordenDTO the ordenDTO to set
	 */
	public void setOrdenDTO(OrdenDTO ordenDTO) {
		this.ordenDTO = ordenDTO;
	}

	/**
	 * @return the tecnicoDTO
	 */
	public TecnicoDTO getTecnicoDTO() {
		return tecnicoDTO;
	}

	/**
	 * @param tecnicoDTO the tecnicoDTO to set
	 */
	public void setTecnicoDTO(TecnicoDTO tecnicoDTO) {
		this.tecnicoDTO = tecnicoDTO;
	}

	/**
	 * @return the viatico
	 */
	public Double getViatico() {
		return viatico;
	}

	/**
	 * @param viatico the viatico to set
	 */
	public void setViatico(Double viatico) {
		this.viatico = viatico;
	}
	
}
