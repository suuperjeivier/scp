package herramientas;

import java.sql.Blob;

import javax.servlet.http.Part;

public class ImagenDTO {
	private int idImagen;
	private Blob imagenBlob;
	private Part imgPart;
	private String imgStringBase64;
	private byte[] imgByteArray;
	public String getImgStringBase64() {
		return imgStringBase64;
	}
	public void setImgStringBase64(String imgStringBase64) {
		this.imgStringBase64 = imgStringBase64;
	}
	public Blob getImagenBlob() {
		return imagenBlob;
	}
	public void setImagenBlob(Blob imagenBlob) {
		this.imagenBlob = imagenBlob;
	}
	public byte[] getImgByteArray() {
		return imgByteArray;
	}
	public void setImgByteArray(byte[] imgByteArray) {
		this.imgByteArray = imgByteArray;
	}
	public int getIdImagen() {
		return idImagen;
	}
	public void setIdImagen(int idImagen) {
		this.idImagen = idImagen;
	}
	/**
	 * @return the imgPart
	 */
	public Part getImgPart() {
		return imgPart;
	}
	/**
	 * @param imgPart the imgPart to set
	 */
	public void setImgPart(Part imgPart) {
		this.imgPart = imgPart;
	}
	

}
