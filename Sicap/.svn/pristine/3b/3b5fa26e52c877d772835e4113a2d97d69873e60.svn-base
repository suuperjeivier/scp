	/**
	 * 
	 * @param img
	 * @return String
	 */ 
	public String imgBlobToStringBase64(Blob img){
		String stringBase64 = null;
		if(img != null){
			byte[] imgArray = null;
			try {
				imgArray = img.getBytes(1, (int) img.length());
			} catch (SQLException e) {				
				e.printStackTrace();
			}
			if(imgArray != null && imgArray.length > 0 && !imgArray.toString().isEmpty()){
				StringBuilder sb = new StringBuilder();
				sb.append("data:image/png;base64,");
				sb.append(StringUtils.newStringUtf8(Base64.encodeBase64(imgArray, false)));
				stringBase64 = sb.toString();
			}else{
				System.out.println("imgArray == null || imgArray <= 0 in imgBlobToStringBase64");
			}                 
		}else{
			System.out.println("blob == null in imgBlobToStringBase64");
		}
		return stringBase64;
	}
	
	
	
