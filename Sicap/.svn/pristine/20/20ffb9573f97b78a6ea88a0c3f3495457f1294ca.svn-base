package dto.asignacion;

import dto.movimientos.MovimientoItemDTO;
import dto.partner.PartnerDTO;

public class AsignacionDTO {
	
	
private int idAsignacion;
private MovimientoItemDTO movimientoItem;
private PartnerDTO partnerAsignacion;
private int status;

public AsignacionDTO(){
	setMovimientoItem(new MovimientoItemDTO());
	setPartnerAsignacion(new PartnerDTO());
}
/**
 * @return the idAsignacion
 */
public int getIdAsignacion() {
	return idAsignacion;
}
/**
 * @param idAsignacion the idAsignacion to set
 */
public void setIdAsignacion(int idAsignacion) {
	this.idAsignacion = idAsignacion;
}
/**
 * @return the movimientoItem
 */
public MovimientoItemDTO getMovimientoItem() {
	return movimientoItem;
}
/**
 * @param movimientoItem the movimientoItem to set
 */
public void setMovimientoItem(MovimientoItemDTO movimientoItem) {
	this.movimientoItem = movimientoItem;
}
/**
 * @return the partnerAsignacion
 */
public PartnerDTO getPartnerAsignacion() {
	return partnerAsignacion;
}
/**
 * @param partnerAsignacion the partnerAsignacion to set
 */
public void setPartnerAsignacion(PartnerDTO partnerAsignacion) {
	this.partnerAsignacion = partnerAsignacion;
}
/**
 * @return the status
 */
public int getStatus() {
	return status;
}
/**
 * @param status the status to set
 */
public void setStatus(int status) {
	this.status = status;
}

}
