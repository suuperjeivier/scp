package dto.movimientos;

import dto.listados.TipoEstatusDTO;
import dto.partner.PartnerDTO;
import dto.user.UserSimpleDTO;
import herramientas.FechaHoraDTO;

public class MovimientoDTO {
	
	private int idMovimiento;
	private int skuMovimiento;
	private String skuMovimientoString;
	private UserSimpleDTO usuarioCreacionDTO;
	private UserSimpleDTO usuarioActualizacionDTO;
	private FechaHoraDTO fechaHoraCreacionDTO;
	private FechaHoraDTO fechaHoraActualizacionDTO;	
	private PartnerDTO partnerAsignacion;	
	private int fkIdTipoMovimiento;
	private String comments;
	private TipoEstatusDTO statusDTO;
	
	public MovimientoDTO(){
		this.setPartnerAsignacion(new PartnerDTO());		
		this.setFkIdTipoMovimiento(144);
		this.setStatusDTO(new TipoEstatusDTO());
		this.setFechaHoraActualizacionDTO(new FechaHoraDTO());
		this.setFechaHoraCreacionDTO(new FechaHoraDTO());
		this.setUsuarioActualizacionDTO(new UserSimpleDTO());
		this.setUsuarioCreacionDTO(new UserSimpleDTO());
	}
	
	public int getIdMovimiento() {
		return idMovimiento;
	}
	
	public void setIdMovimiento(int idMovimiento) {
		this.idMovimiento = idMovimiento;
	}
	
	public int getSkuMovimiento() {
		return skuMovimiento;
	}
	
	public void setSkuMovimiento(int skuMovimiento) {
		this.skuMovimiento = skuMovimiento;
	}
	
	public int getFkIdTipoMovimiento() {
		return fkIdTipoMovimiento;
	}
	
	public void setFkIdTipoMovimiento(int fkIdTipoMovimiento) {
		this.fkIdTipoMovimiento = fkIdTipoMovimiento;
	}

	public String getSkuMovimientoString() {
		return skuMovimientoString;
	}

	public void setSkuMovimientoString(String skuMovimientoString) {
		this.skuMovimientoString = skuMovimientoString;
	}

	/**
	 * @return the partnerAsignacion
	 */
	public PartnerDTO getPartnerAsignacion() {
		return partnerAsignacion;
	}

	/**
	 * @param partnerAsignacion the partnerAsignacion to set
	 */
	public void setPartnerAsignacion(PartnerDTO partnerAsignacion) {
		this.partnerAsignacion = partnerAsignacion;
	}

	/**
	 * @return the statusDTO
	 */
	public TipoEstatusDTO getStatusDTO() {
		return statusDTO;
	}

	/**
	 * @param statusDTO the statusDTO to set
	 */
	public void setStatusDTO(TipoEstatusDTO statusDTO) {
		this.statusDTO = statusDTO;
	}

	/**
	 * @return the comments
	 */
	public String getComments() {
		return comments;
	}

	/**
	 * @param comments the comments to set
	 */
	public void setComments(String comments) {
		this.comments = comments;
	}

	/**
	 * @return the usuarioCreacionDTO
	 */
	public UserSimpleDTO getUsuarioCreacionDTO() {
		return usuarioCreacionDTO;
	}

	/**
	 * @param usuarioCreacionDTO the usuarioCreacionDTO to set
	 */
	public void setUsuarioCreacionDTO(UserSimpleDTO usuarioCreacionDTO) {
		this.usuarioCreacionDTO = usuarioCreacionDTO;
	}

	/**
	 * @return the usuarioActualizacionDTO
	 */
	public UserSimpleDTO getUsuarioActualizacionDTO() {
		return usuarioActualizacionDTO;
	}

	/**
	 * @param usuarioActualizacionDTO the usuarioActualizacionDTO to set
	 */
	public void setUsuarioActualizacionDTO(UserSimpleDTO usuarioActualizacionDTO) {
		this.usuarioActualizacionDTO = usuarioActualizacionDTO;
	}

	/**
	 * @return the fechaHoraCreacionDTO
	 */
	public FechaHoraDTO getFechaHoraCreacionDTO() {
		return fechaHoraCreacionDTO;
	}

	/**
	 * @param fechaHoraCreacionDTO the fechaHoraCreacionDTO to set
	 */
	public void setFechaHoraCreacionDTO(FechaHoraDTO fechaHoraCreacionDTO) {
		this.fechaHoraCreacionDTO = fechaHoraCreacionDTO;
	}

	/**
	 * @return the fechaHoraActualizacionDTO
	 */
	public FechaHoraDTO getFechaHoraActualizacionDTO() {
		return fechaHoraActualizacionDTO;
	}

	/**
	 * @param fechaHoraActualizacionDTO the fechaHoraActualizacionDTO to set
	 */
	public void setFechaHoraActualizacionDTO(FechaHoraDTO fechaHoraActualizacionDTO) {
		this.fechaHoraActualizacionDTO = fechaHoraActualizacionDTO;
	}
	
	

}
